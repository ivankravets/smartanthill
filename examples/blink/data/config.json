{
  "services":
  {
    "network":
    {
      "enabled": true,
      "options":
      {
        "routers":
        [
          {
            "type": "serial",
            "port": "/dev/tty.SLAB_USBtoUART",
            "baudrate": 9600,
            "deviceids": [128]
          }
        ]
      }
    },

    "device":
    {
      "enabled": true,
      "options":
      {
        "128":
        {
          "board": "arduino_pro5v",
          "operations":
          [
            "ping",
            "listoperationalstates",
            "configurepinmode",
            "readdigitalpin",
            "writedigitalpin",
            "configureanalogreference",
            "readanalogpin"
          ]
        }
      }
    }
  }
}
